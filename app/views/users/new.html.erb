<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br></br>
  <%= f.label :height %>
  <%= f.text_field :height %>
  <br></br>
  <%= f.label :happiness %>
  <%= f.text_field :happiness %>
  <br></br>
  <%= f.label :nausea %>
  <%= f.text_field :nausea %>
  <br></br>
  <%= f.label :tickets %>
  <%= f.text_field :tickets %>
  <br></br>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <br></br>
  <%= f.label :admin, "Is this an admin account?" %>
  <%= f.check_box:admin, { class: "user_admin" }, true %>
  <br></br>
  <%= f.submit "Create User" %>
<% end %>
